# Where Statement  "="

•	For Selecting any Specific kind of value

****Query: Select *From dbo.EmployeeDemographics Where Age = '30';


#Where Statement "<>"

•	Does Not Equal [<>]

*** Query: Select * From dbo.EmployeeDemographics where Age <> '30';


#Where Statement: ">"

•	>

***Query: Select * From dbo.EmployeeDemographics where Age > '30';


#Where Statement: "<"

•	<

***Query: Select * From dbo.EmployeeDemographics where Age < '30';



#Where Statement: ">="

•>=

***Query: Select * From dbo.EmployeeDemographics where Age >= '30';



#Where Statement: 'AND'

•	AND

***Query: Select * From dbo.EmployeeDemographics where Age >'30' and  Gender = 'Female';



#Where Statement: 'OR'

•	OR

***Query: Select * From dbo.EmployeeDemographics where Age >'30' or Gender = 'Female';



#Where Statement: 'LIKE'

•	LIKE : For Selecting any name start with any alphabet

***Query: Select * From dbo.EmployeeDemographics where LastName Like 's%' ;

•	 For Selecting any name with any Specific alphabet: 

*** Query: Select * From dbo.EmployeeDemographics where LastName Like '%s%' ;
***Here % called wildcard


# Where Statement: NULL

•	 NULL: For identifying any null value

***Query: Select * From dbo.EmployeeDemographics where LastName is NULL;


# Where Statement: NOT NULL

•	NOT NULL: For identifying any not null value

***Query: Select * From dbo.EmployeeDemographics where LastName is Not NULL;


# Where Statement: IN

•	IN: In is kind of like = statement but it is a multiple equal statement

***Query: Select *From dbo.EmployeeDemographics where FirstName In ('Pam' , 'Jim');


